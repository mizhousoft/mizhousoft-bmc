<?xml version="1.0" encoding="UTF-8"?>
<configuration>

	<appenders>

		<console name="Console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS}|%-5level|%t|%c{5}.%L|%msg%xEx%n" />
		</console>

	</appenders>

	<loggers>
		<root level="info">
			<appender-ref ref="Console" />
		</root>
	</loggers>

</configuration>